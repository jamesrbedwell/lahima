<h1>Edit Client Contact</h1>
<form action="/client_contacts" method="post">
  <input type="text" name="_method" value="put">
  <label for="">First Name</label>
  <input type="text" name="first_name" value="<%= @client_contact.first_name %>">
  <label for="">Last Name</label>
  <input type="text" name="last_name" value="<%= @client_contact.last_name %>">
  <label for="">Phone Number</label>
  <input type="text" name="phone" value="<%= @client_contact.phone %>">
  <label for="">Email</label>
  <input type="text" name="email" value="<%= @client_contact.email %>">
  <label for="">Client Name</label>
  CREATE A DROP DOWN MENU OF CLIENT NAMES
  <input type="text" name="client_id" value="<%= @client_contact.client_id %>">
  
  <button>Add Client Contact</button>
</form>