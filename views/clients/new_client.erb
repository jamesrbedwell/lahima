<h1>Add a New Client</h1>

<form action="/clients" method="post">
  <label for="">Name</label>
  <input type="text" name="name">
  <label for="">ABN</label>
  <input type="text" name="abn">
  <label for="">Street Address</label>
  <input type="text" name="address1">
  <label for="">City</label>
  <input type="text" name="city">
  <label for="">State</label>
  <input type="text" name="state">
  <label for="">Country</label>
  <input type="text" name="country">
  <label for="">Post Code</label>
  <input type="text" name="post_code">
  <label for="">Labour Rate</label>
  <input type="number" name="labour_rate">
  <label for="">Ticketed Rate</label>
  <input type="number" name="ticketed_rate">
  <label for="">Trade Rate</label>
  <input type="number" name="trade_rate">
  <button>Add Client</button>
</form>


